{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "sonarjs"
  ],
  "extends": [
    "google",
    "plugin:sonarjs/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["warn", { "code": 100 }]
  }
}
